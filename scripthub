local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Keva Hub", "Midnight")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Universal ESP", "Execute this to get ESP.", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/TheCatsYT/robloxscripts/main/Universal%20ESP'))()
end)
